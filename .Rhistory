library(PPBstats)
# test GxE
d1 = design_experiment(
location = "L1",
year = "2016",
expe.type = "fully-replicated",
germplasm = paste("germ", c(1:20), sep = ":"),
nb.blocks = 3,
nb.cols = 4)
d1 = d1$`fully-replicated`$data.frame
d1$y1 = rnorm(nrow(d1), 50, 5)
d1$y2 = rnorm(nrow(d1), 30, 5)
d1$y3 = rnorm(nrow(d1), 60, 5)
d2 = design_experiment(
location = "L2",
year = "2016",
expe.type = "fully-replicated",
germplasm = paste("germ", c(1:20), sep = ":"),
nb.blocks = 3,
nb.cols = 4)
d2 = d2$`fully-replicated`$data.frame
d2$y1 = rnorm(nrow(d2), 40, 10)
d2$y2 = rnorm(nrow(d2), 20, 10)
d2$y3 = rnorm(nrow(d2), 50, 10)
d3 = design_experiment(
location = "L3",
year = "2016",
expe.type = "fully-replicated",
germplasm = paste("germ", c(1:20), sep = ":"),
nb.blocks = 3,
nb.cols = 4)
d3 = d3$`fully-replicated`$data.frame
d3$y1 = rnorm(nrow(d3), 30, 2)
d3$y2 = rnorm(nrow(d3), 10, 2)
d3$y3 = rnorm(nrow(d3), 40, 2)
d = rbind.data.frame(d1, d2, d3)
vec_variables = c("y1", "y2", "y3")
gxe_analysis = "AMMI"
PPBdata_GxE = d
library(knitr)
